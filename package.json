{
  "name": "dokan-pro",
  "version": "3.1.0",
  "description": "A WordPress marketplace plugin",
  "author": "weDevs",
  "license": "GPL",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --progress --colors --watch --hide-modules",
    "dev:build": "cross-env NODE_ENV=development webpack",
    "build": "cross-env NODE_ENV=production webpack",
    "zip": "node dist/zip",
    "zip-upload": "node dist/zip-upload",
    "version": "node dist/version-replace",
    "clean": "node dist/clean",
    "makepot": "node dist/makepot",
    "release": "npm run clean && npm run dev:build && npm run build && npm run makepot && npm run version && npm run zip"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@wordpress/babel-preset-default": "^4.19.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "file-loader": "^5.1.0",
    "fs-extra": "^8.1.0",
    "less": "^3.12.2",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "replace-in-file": "^5.0.2",
    "vue-loader": "^15.9.5",
    "vue-star-rating": "^1.7.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "wp-vue-i18n": "^1.1.4"
  }
}
